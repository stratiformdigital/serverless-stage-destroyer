{
  "name": "@stratiformdigital/serverless-stage-destroyer",
  "version": "1.7.0",
  "description": "Deletes all Cloudformation Stacks associated to a specified Serverless stage.",
  "main": "./dist/index.js",
  "typings": "./dist/index.js",
  "scripts": {
    "test": "npm run gen && node tests/run.mjs",
    "tslint": "tslint -p ./",
    "bp": "npm run tslint && rm -rf ./dist && rm -rf ./node_modules && npm i --save && tsc && npm publish",
    "gen": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stratiformdigital/serverless-stage-destroyer"
  },
  "keywords": [
    "Serverless Stage Destroyer",
    "Serverless"
  ],
  "author": "mdial89f",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.128.0",
    "@aws-sdk/client-s3": "^3.128.0",
    "@types/readline-sync": "^1.4.4",
    "readline-sync": "^1.4.10"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@serverless/compose": "^1.2.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.0.4",
    "jest": "^27.5.1",
    "lodash": "^4.17.21",
    "semantic-release": "^19.0.3",
    "serverless": "^3.21.0",
    "ts-jest": "^27.1.4",
    "tslint": "^6.1.3",
    "tslog": "^3.3.3",
    "typescript": "^4.7.4"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
