{
  "name": "serverless-stage-destroyer",
  "version": "1.0.0",
  "description": "Deletes all Cloudformation Stacks associated to a specified Serverless stage.",
  "main": "./dist/index.js",
  "typings": "./dist/index.js",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "tslint": "tslint -p ./",
    "bp": "npm run tslint && rm -rf ./dist && rm -rf ./node_modules && npm i --save && tsc && npm publish",
    "gen": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mdial89f/serverless-stage-destroyer"
  },
  "keywords": [
    "Serverless Stage Destroyer",
    "Serverless"
  ],
  "author": "mdial89f",
  "license": "MIT",
  "husky": {},
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.48.0",
    "@aws-sdk/client-s3": "^3.48.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/readline-sync": "^1.4.4",
    "jest": "^23.0.0",
    "readline-sync": "^1.4.10",
    "ts-jest": "^23.10.5",
    "tslog": "^3.3.3"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^18.0.1"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
